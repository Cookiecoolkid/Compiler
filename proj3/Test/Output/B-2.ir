temp3 := gridResult_ / #10
temp4 := temp3 * #10
IF temp4 == gridResult_ GOTO label0
GOTO label1
LABEL label0 :
temp5 := gridResult_ / #10
RETURN temp5
GOTO label2
LABEL label1 :
temp6 := gridResult_ + #5
RETURN temp6
LABEL label2 :

FUNCTION main :
